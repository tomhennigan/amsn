	<General>
		<Colors>
			<balloonbackground>#F2F2F2</balloonbackground> <!-- balloon is infobox what you can see when you hold mouse upper of contact-->
			<balloonborder>#000000</balloonborder>
			<balloontext>#000000</balloontext>
		</Colors>
		<Geometry>
	        <notifwidth>202</notifwidth>
			<notifheight>134</notifheight>
			<x_notifyclose>184</x_notifyclose>
			<y_notifyclose>3</y_notifyclose>
			<x_notifydp>13</x_notifydp>
			<y_notifydp>45</y_notifydp>
			<x_notifytext>75</x_notifytext>
			<y_notifytext>47</y_notifytext>
			<width_notifytext>121</width_notifytext>
			<notify_font>sboldf</notify_font>
			<notify_dp_border>1</notify_dp_border>
		</Geometry>
		<!--Note: On linux, balloonborderwidth 0 gives a border of 1px due to tk platform differences. To have no balloon border, set balloonborderwidth to 0 and have the balloonborder the same colour as the balloonbackground-->
		<Options>
			<balloonfont>sboldf</balloonfont>
			<balloonborderwidth>0</balloonborderwidth>
		</Options>
	</General>
