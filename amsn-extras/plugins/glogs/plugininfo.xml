<?xml version="1.0"?>
<plugin>
	<name>GLogs</name>
	<author>Ignacio Larrain</author>
	<description>Send logs of conversations to your email. It was made to take advantage of GMail features, but can be used with any mail account.</description>
	<amsn_version>0.95</amsn_version>
	<plugin_version>1</plugin_version>
	<plugin_file>glogs.tcl</plugin_file>
	<plugin_namespace>glogs</plugin_namespace>
	<init_procedure>InitPlugin</init_procedure>
	
	<cvs_version>1.6</cvs_version>
	
	<file>
		<path>smtp.tcl</path>
		<version>1.2</version>
	</file>
	
	<lang>
		<langcode>en</langcode>
		<version>1.1</version>
	</lang>

	<lang>
		<langcode>es</langcode>
		<version>1.1</version>
	</lang>

	<lang>
		<langcode>fr</langcode>
		<version>1.1</version>
	</lang>
	<lang>
		<langcode>da</langcode>
		<version>1.1</version>
	</lang>

	<URL>
		<main>http://amsn.sourceforge.net/autoupdater/plugins/glogs/glogs.tcl</main>
		<plugininfo>http://amsn.sourceforge.net/autoupdater/plugins/glogs/plugininfo.xml</plugininfo>
		<lang>http://amsn.sourceforge.net/autoupdater/plugins/glogs/lang/lang$langcode</lang>
		<file>http://amsn.sourceforge.net/autoupdater/plugins/glogs/$file</file>
	</URL>

</plugin>
