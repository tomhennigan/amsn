	<cvs_version>1.10</cvs_version>
	
	<lang>
		<langcode>en</langcode>
		<version>1.4</version>
	</lang>
	
	<lang>
		<langcode>fr</langcode>
		<version>1.4</version>
	</lang>
	
	<lang>
		<langcode>fr_CA</langcode>
		<version>1.3</version>
	</lang>

	<lang>
		<langcode>ee</langcode>
		<version>1.2</version>
	</lang>
	
	<file>
		<path>display_and_send.scpt</path>
		<version>1.5</version>
	</file>
	
	<file>
		<path>infoamarok</path>
		<version>1.2</version>
	</file>	
</plugin>
