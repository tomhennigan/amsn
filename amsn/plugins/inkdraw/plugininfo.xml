<?xml version="1.0"?>
<plugin>
	<name>Inkdraw</name>
	<author>Karel Demeyer</author>
	<description>With this plugin you can draw in the chatwindow and send your drawings/sketches to your contacts.  It implements the ink feature found in MSN Messenger 7+</description>
	<amsn_version>0.95</amsn_version>
	<plugin_version>0.2</plugin_version>
	<plugin_file>inkdraw.tcl</plugin_file>
	<plugin_namespace>draw</plugin_namespace>
	<init_procedure>Init</init_procedure>
	<cvs_version>1.7</cvs_version>

	<URL>
		<main>http://www.amsn-project.net/autoupdater/plugins2/inkdraw/inkdraw.tcl</main>
		<plugininfo>http://www.amsn-project.net/autoupdater/plugins2/inkdraw/plugininfo.xml</plugininfo>
		<lang>http://www.amsn-project.net/autoupdater/plugins2/inkdraw/lang/lang$langcode</lang>
		<file>http://www.amsn-project.net/autoupdater/plugins2/inkdraw/$file</file>
	</URL>

</plugin>
