	<General>
		<Colors>
			<balloonbackground>#296da4</balloonbackground>
			<balloonborder>#000000</balloonborder>
			<balloontext>#ffffff</balloontext>
		</Colors>
		<Geometry>
	            <notifwidth>195</notifwidth>
			<notifheight>134</notifheight>
			<x_notifyclose>180</x_notifyclose>
			<y_notifyclose>2</y_notifyclose>
			<x_notifydp>5</x_notifydp>
			<y_notifydp>45</y_notifydp>
			<x_notifytext>60</x_notifytext>
			<y_notifytext>45</y_notifytext>
			<width_notifytext>125</width_notifytext>
			<notify_font>sboldf</notify_font>
		</Geometry>
		<!--Note: On linux, balloonborderwidth 0 gives a border of 1px due to tk platform differences. To have no balloon border, set balloonborderwidth to 0 and have the balloonborder the same colour as the balloonbackground-->
		<Options>
			<balloonfont>sboldf</balloonfont>
			<balloonborderwidth>0</balloonborderwidth>
		</Options>
	</General>
